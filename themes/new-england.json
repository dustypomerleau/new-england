{
  "name": "New England light theme",
  "type": "light",
  "tokenColors": [
    {
      "name": "comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8D867B"
      }
    },
    {
      "name": "yellow",
      "scope": [
        "storage.type.java",
        "variable.other.readwrite",
        "constant.type-constructor.elm",
        "keyword.control.at-rule",
      ],
      "settings": {
        "foreground": "#664400"
      }
    },
    {
      "name": "red: constants, variables, tags",
      "scope": [
        "constant.language",
        "constant.numeric",
        "entity.name.tag",
        "markup.changed.git_gutter",
        "markup.deleted.git_gutter",
        "markup.fenced_code.block.markdown",
        "markup.inline.raw.string.markdown",
        "markup.raw.block.fenced.markdown",
        "markup.raw.block",
        "support.class",
        "support.function.general.tex",
        "text.html.markdown markup.inline.raw.markdown",
        "variable.other.constant", // navy for JS/TS
        "variable.other.object",
        "variable.type.elm",
        "support.constant.property-value.css",
      ],
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "slate: classes, object literal keys",
      "scope": [
        "entity.name.type.class",
        "entity.name.type.module",
        "entity.name.class",
        "entity.name.section",
        "entity.other.inherited-class",
        "keyword.control.preamble.latex",
        "variable.language",
        "support.module.elm",
      ],
      "settings": {
        "foreground": "#587182"
      }
    },
    {
      "name": "navy: properties, variables, methods",
      "scope": [
        "constant",
        "entity.name.record.field.accessor.elm",
        "keyword.other.unit",
        "meta",
        "storage.modifier.import.java",
        "variable.other.constant.js",
        "variable.other.constant.jsx",
        "variable.other.constant.ts",
        "variable.other.constant.tsx",
        "variable",
      ],
      "settings": {
        "foreground": "#3e454e"
      }
    },
    {
      "name": "invalid",
      "scope": [
        "invalid.deprecated",
        "invalid.illegal",
        "invalid"
      ],
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "orange: keyword, control, storage",
      "scope": [
        "keyword.control.newline.tex",
        "keyword",
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown",
        "markup.heading.setext",
        "markup.quote punctuation.definition.blockquote.markdown",
        "meta.separator",
        "meta.structure.tuple.erlang",
        "punctuation.definition.constant.ruby",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.raw.markdown",
        "variable.other.anonymous.elixir punctuation.definition.variable.elixir",
        "punctuation.definition.constant.elixir",
        "punctuation.separator.clause-head-body.erlang",
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.separator.key-value",
        "punctuation.type.flowtype",
        "storage",
        "support.function.be.latex",
        "support.function.section.latex",
        "support.function.textbf.latex",
        "support.function.textit.latex",
        "support.function.url.latex",
        "entity.other.attribute-name.class.css",
      ],
      "settings": {
        "foreground": "#9a4e32"
      }
    },
    {
      "name": "brown: punctuation",
      "scope": [
        "meta.tag",
        "punctuation.definition.fenced.markdown",
        "punctuation.definition",
        "punctuation",
      ],
      "settings": {
        "foreground": "#585550"
      }
    },
    {
      "name": "pink: interpolations, regular expressions",
      "scope": [
        "constant.numeric.math.tex",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.quasi.element.begin.js",
        "punctuation.quasi.element.end.js",
        "punctuation.section.embedded",
        "string.other.link.description.markdown",
        "string.other.link.title.markdown",
        "string.regexp",
        "string.regexp keyword",
        "string.regexp meta.group",
        "string.regexp meta keyword",
        "string.regexp constant",
        "string.regexp constant.character",
        "string.regexp meta punctuation meta.assertion",
        "string.regexp meta punctuation",
        "string.regexp punctuation",
        "support.class.latex",
        "support.class.math.latex",
        "variable.interpolation",
        "storage.type.format.python",
        "entity.other.attribute-name.id.css",
      ],
      "settings": {
        "foreground": "#93255b"
      }
    },
    {
      "name": "sea green: functions",
      "scope": [
        "entity.name.function",
        "entity.name.section.latex",
        "keyword.control.layout.tex",
        "keyword.control.ref.latex",
        "meta.function.embedded.latex",
        "support.function.magic.python",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
      ],
      "settings": {
        "foreground": "#2c5851"
      }
    },
    {
      "name": "purple: atoms, function params, list items, types",
      "scope": [
        "constant.language.symbol",
        "constant.other.reference.label.latex",
        "constant.other.reference.link.markdown",
        "constant.other.symbol", // atoms
        "markup.list meta.paragraph.markdown",
        "meta.function.parameters variable.other.readwrite",
        "punctuation.section.list.begin.elixir",
        "punctuation.section.list.end.elixir",
        "punctuation.section.regexp.begin.elixir",
        "punctuation.section.regexp.end.elixir",
        "storage.type.elm",
        "variable.other.object.js",
        "variable.parameter",
        "entity.name.tag.css",
      ],
      "settings": {
        "foreground": "#490049"
      }
    },
    {
      "name": "forest green: strings",
      "scope": [
        "markup.heading",
        "markup.inserted.git_gutter",
        "string",
        "variable.parameter.url.css",
      ],
      "settings": {
        "foreground": "#5b5c31"
      }
    },
    {
      "name": "blue: keys, objects, module attributes",
      "scope": [
        "constant.other.general.math.tex",
        "entity.name.record.field.elm",
        "entity.other.attribute-name",
        "meta.function-call entity.name.function",
        "meta.method-call entity.name.function",
        "meta.object-literal.key",
        "meta.object.flowtype storage.type.function",
        "meta.object.flowtype variable.other.readwrite",
        "meta.property-name",
        "string.other.link.description.title.markdown",
        "support.type.property-name",
      ],
      "settings": {
        "foreground": "#43596f"
      }
    },
    {
      "name": "teal: escapes, placeholders",
      "scope": [
        "constant.character.escape",
        "constant.character.numeric.regexp",
        "constant.character",
        "string constant.other.placeholder",
      ],
      "settings": {
        "foreground": "#407875"
      }
    },
    {
      "name": "JS method names",
      "scope": [
        "entity.name.method.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#3e454e"
      }
    },
    {
      "name": "this",
      "scope": [
        "variable.language.this",
        "variable.language.special.self.python",
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#93255b"
      }
    },
    {
      "name": "HTML attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#3e454e"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "markdown italic delimiters, ES7 bind operator",
      "scope": [
        "punctuation.definition.italic.markdown",
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9a4e32"
      }
    },
    {
      "name": "markup italic text",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#43596f"
      }
    },
    {
      "name": "markup bold text",
      "scope": [
        "markup.bold",
        "markup.bold string",
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#43596f"
      }
    },
    {
      "name": "markdown bold delimiters",
      "scope": [
        "punctuation.definition.bold.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#9a4e32"
      }
    },
    {
      "name": "markup underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#5b5c31"
      }
    },
    {
      "name": "markup block quote",
      "scope": [
        "markup.quote meta.paragraph.markdown"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2c5851"
      }
    },
    {
      "name": "markup inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "markup deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#dc322f80"
      }
    },
    {
      "name": "markup changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#43596f"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#407875"
      }
    }
  ],
